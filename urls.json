	"amazon": ["(https://(www\\.)?amazon\\.[a-z.]*/(.*/dp|dp|gp/product)/[A-Z0-9]*).*", "$1"],
	"ebay": ["(https://(www\\.)?ebay.(.*)/itm/[0-9]*).*", "$1"],
